  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.0.0-beta.2",
    "js-cookie": "^2.2.1",
    "parcel": "^2.0.0-beta.2",
    "quill": "1.3.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  }
